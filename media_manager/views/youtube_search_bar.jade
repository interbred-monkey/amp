div(class='span6', box)
  div(class='overflow ml40 pad10')
    form(class='navbar-search form-inline', autocomplete='off', onsubmit='return false')
      label(class='control-label mr20') Find a YouTube Video
      input(type='text', id='youtube_search', class='search-query typeahead input-xxlarge', placeholder='Foo Fighters')
      button(type='button', class='btn btn-primary ml30', onclick='$("#youtube_search").keyup()') Search
