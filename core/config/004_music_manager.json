[
  {
    "title": "Gets similar artist information from last.fm",
    "method": "GET",
    "endpoint": "/ajax/similar-artists",
    "module": "music_manager_display.js",
    "callback": "displaySimilarArtistInfo"
  },
  {
    "title": "Gets artist information from last.fm",
    "method": "GET",
    "endpoint": "/ajax/artist-info",
    "module": "music_manager_display.js",
    "callback": "displayArtistInfo"
  },
  {
    "title": "Get a music group from the library",
    "method": "GET",
    "endpoint": "/ajax/music-([A-Za-z0-9-_]+)-list",
    "module": "music_manager_display.js",
    "callback": "displayGroupedList"
  },
  {
    "title": "Get the songs from a group in the music library",
    "method": "GET",
    "endpoint": "/ajax/music-subset",
    "module": "music_manager_display.js",
    "callback": "displayGroupSubset"
  },
  {
    "title": "Get all the songs in the music library",
    "method": "GET",
    "endpoint": "/ajax/music-all",
    "module": "music_manager_display.js",
    "callback": "displaySongs"
  },
  {
    "title": "Start a re-index",
    "method": "POST",
    "endpoint": "/ajax/reindex-music",
    "module": "music_manager.js",
    "callback": "reindex"
  }
]